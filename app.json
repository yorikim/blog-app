{
  "expo": {
    "name": "Blog app",
    "slug": "blog-app",
    "scheme": "blogapp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.yorikim.blog",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.yorikim.blog"
    },
    "description": ""
  }
}
